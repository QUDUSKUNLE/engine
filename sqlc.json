{
  "version": "2",
  "sql": [{
    "schema": "adapters/db/migrations",
    "queries": "db/queries",
    "engine": "postgresql",
    "gen": {
      "go": {
        "package": "db",
        "out": "adapters/db",
        "emit_json_tags": true,
        "emit_prepared_queries": false,
        "emit_interface": true,
        "emit_exact_table_names": false,
        "emit_empty_slices": true
      }
    }
  }]
}
