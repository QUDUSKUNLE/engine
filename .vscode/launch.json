{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Go (main.go)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {},
            "args": [],
            "showLog": true
        },
        {
            "name": "Attach to Go (port 7556)",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "port": 7556,
            "host": "127.0.0.1",
            "showLog": true
        }
    ]
}
